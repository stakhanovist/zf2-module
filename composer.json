{
  "name": "stakhanovist/zf2-module",
  "description": "ZF2 module for stakhanovist library",
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "Leonardo Grasso",
      "email": "me@leonardograsso.com",
      "homepage": "http://github.com/leogr"
    },
    {
      "name": "Leonardo Di Donato",
      "email": "leodidonato@gmail.com",
      "homepage": "http://github.com/leodido"
    }
  ],
  "require": {
    "php": ">=5.5",
    "stakhanovist/worker": "0.2.*@dev",
    "stakhanovist/queue": "0.2.*@dev",
    "zendframework/zend-mvc": "~2.4",
    "zendframework/zend-servicemanager": "~2.4",
    "zendframework/zend-http": "~2.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.2",
    "satooshi/php-coveralls": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Stakhanovist\\Module\\": "src/"
    },
    "classmap": [
      "./Module.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "StakhanovistModuleTest\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev",
      "dev-develop": "0.2.x-dev"
    }
  },
  "bin": [
    "bin/stakhanovist"
  ]
}
