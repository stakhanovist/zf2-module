{
    "name": "stakhanovist/zf2-module",
    "description": "ZF2 module for stakhanovist library",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Leonardo Grasso",
            "email": "me@leonardograsso.com",
            "homepage": "http://github.com/leogr"
        }
    ],
    "require": {
        "php": ">=5.4",
        "stakhanovist/worker": "0.2.*@dev",
        "stakhanovist/queue": "0.2.*@dev",
        "zendframework/zendframework": "~2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
        	"Stakhanovist\\Module\\": "src/",
        	"StakhanovistModuleTest\\": "tests/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev",
            "dev-develop": "0.2.x-dev"
        }
    }
}
